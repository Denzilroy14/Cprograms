//C program to swap two variables using pointers
#include <stdio.h>
int main() {
    int *c,a,b,temp;
    printf("Enter value of a:");
    scanf("%d",&a);
    printf("Enter value of b:");
    scanf("%d",&b);
    c=a;
    a=b;
    temp=c;
    b=temp;
    printf("%d",a);
    printf("\n%d",b);
    return 0;
}
